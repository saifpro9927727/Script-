-- 1. Main Hub Tabs
for gameName, scripts in pairs(GamesScripts) do
    if gameName ~= "üìù Custom Script" and gameName ~= "‚≠ê Credits" then
        local tab = Window:MakeTab({
            Name = gameName,
            Icon = "rbxassetid://4483345998",
            PremiumOnly = false
        })
        for _, script in ipairs(scripts) do
            tab:AddButton({
                Name = script.Name .. (script.Key and " (Key Required)" or " (No Key)"),
                Callback = function()
                    local success, result = pcall(function()
                        return loadstring(game:HttpGet(script.Url))()
                    end)
                    if success then
                        OrionLib:MakeNotification({
                            Name = script.Name .. " Loaded",
                            Content = "Script executed for " .. gameName,
                            Image = "rbxassetid://4483345998",
                            Time = 3
                        })
                    else
                        OrionLib:MakeNotification({
                            Name = "Execution Failed",
                            Content = "Could not load script: "..script.Name,
                            Image = "rbxassetid://4483345998",
                            Time = 3
                        })
                    end
                end
            })
        end
    end
end

-- 2. Custom Script Executor Tab
local customTab = Window:MakeTab({
    Name = "üìù Custom Script",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local customScript = ""
customTab:AddTextbox({
    Name = "Paste Script Here",
    Default = "",
    TextDisappear = false,
    Callback = function(Value)
        customScript = Value
    end
})
customTab:AddButton({
    Name = "Execute Custom Script",
    Callback = function()
        if customScript and customScript ~= "" then
            local success, result = pcall(function()
                return loadstring(customScript)()
            end)
            if success then
                OrionLib:MakeNotification({
                    Name = "Custom Script Executed",
                    Content = "Your custom script has been executed.",
                    Image = "rbxassetid://4483345998",
                    Time = 3
                })
            else
                OrionLib:MakeNotification({
                    Name = "Execution Failed",
                    Content = "Could not execute custom script.",
                    Image = "rbxassetid://4483345998",
                    Time = 3
                })
            end
        else
            OrionLib:MakeNotification({
                Name = "No Script",
                Content = "Paste a script first!",
                Image = "rbxassetid://4483345998",
                Time = 3
            })
        end
    end
})

-- 3. Credits Tab
local creditsTab = Window:MakeTab({
    Name = "‚≠ê Credits",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
creditsTab:AddParagraph("Hub by", "Saif_gamer")
creditsTab:AddParagraph("UI Library", "OrionLib by jensonhirst")
creditsTab:AddParagraph("Scripts", "rScripts, ScriptBlox, and original authors.\nThis is a free, educational project.\n\nIf you like this hub, share it with friends!\n\nGitHub: github.com/saifpro9927727")

OrionLib:MakeNotification({
    Name = "Sigma Boys Ultimate GIGA Hub",
    Content = "Loaded! Thanks for using. Credits in the ‚≠ê tab.",
    Image = "rbxassetid://4483345998",
    Time = 5
})

